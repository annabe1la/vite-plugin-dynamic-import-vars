    "test": "vitest",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/estree": "^0.0.51",
    "@types/node": "^17.0.35",
    "estree-walker": "^3.0.1",
