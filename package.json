  },
  "keywords": [
    "dynamic-import",
    "vite",
    "plugin",
    "dynamic-import-module",
    "dynamic-import-vars",
    "rollup"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "play": "pnpm run --filter ./playground dev",
